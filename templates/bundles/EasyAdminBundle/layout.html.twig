{% extends '@!EasyAdmin/layout.html.twig' %}

{% block head_favicon %}
	<link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}?v={{ date().timestamp }}">
	<link rel="icon" type="image/png" sizes="32x32" href="{{ asset('favicon-32x32.png') }}?v={{ date().timestamp }}">
	<link rel="icon" type="image/png" sizes="16x16" href="{{ asset('favicon-16x16.png') }}?v={{ date().timestamp }}">
	<link rel="apple-touch-icon" href="{{ asset('apple-touch-icon.png') }}?v={{ date().timestamp }}">
{% endblock %}
