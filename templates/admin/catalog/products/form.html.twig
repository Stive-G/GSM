{% extends '@EasyAdmin/page/content.html.twig' %}

{% block page_title title %}

{% block content %}
    {{ form_start(form) }}
    {{ form_row(form.name) }}
    {{ form_row(form.slug) }}
    {{ form_row(form.description) }}
    {{ form_row(form.brand) }}
    {{ form_row(form.categories) }}
    {{ form_row(form.variants) }}
    {{ form_row(form.isActive) }}
    <button class="btn btn-primary">Enregistrer</button>
    <a class="btn btn-secondary" href="{{ path('admin_catalog_products_index') }}">Retour</a>
    {{ form_end(form) }}
{% endblock %}
